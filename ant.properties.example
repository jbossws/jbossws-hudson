#
# A sample ant properties file
#

# JBoss remote repository
jboss.repository=http://repository.jboss.org

# The JBoss settings
jboss.bind.address=localhost

# JDK settings
java.home.sun.jdk7=/usr/java/jdk1.7
java.home.sun.jdk8=/usr/java/jdk1.8
java.home.sun.jdk9=/usr/java/jdk1.9

