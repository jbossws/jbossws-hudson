#
# A sample ant properties file
#

# JBoss remote repository
jboss.repository=http://repository.jboss.org

# The JBoss settings
jboss.bind.address=localhost

# JDK settings
java.home.sun.jdk8=/usr/java/jdk1.8
java.home.oracle.jdk10=/usr/java/jdk10

# Hudson QA Environment
hudson.root=/data/workspace
hudson.host=jbossws-qa.jboss.org
hudson.admin.port=8150
hudson.http.port=8180

hudson.cxf.url=https://github.com/jbossws/jbossws-cxf
hudson.cxf.rev=master

hudson.jboss1100.url=https://github.com/wildfly/wildfly
hudson.jboss1100.build=wildfly-11.0.0.Final
hudson.jboss1100.rev=11.0.0.Final

hudson.jboss1200.url=https://github.com/wildfly/wildfly
hudson.jboss1200.build=wildfly-12.0.0.Final
hudson.jboss1200.rev=12.0.0.Final

hudson.jboss1300.url=https://github.com/wildfly/wildfly
hudson.jboss1300.build=wildfly-13.0.0.Final
hudson.jboss1300.rev=13.0.0.Final

hudson.jboss1400.url=https://github.com/wildfly/wildfly
hudson.jboss1400.build=wildfly-14.0.0.Beta1-SNAPSHOT
hudson.jboss1400.rev=master

hudson.mail.recipients=
hudson.smtp.host=localhost

# Hudson setup
apache-tomcat=5.5.31
git-plugin=2.4.0
git-client-plugin=1.19.0
scm-api-plugin=0.2
performance-plugin=1.13
jenkins-ver=1.650
